  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <meta name="keywords" content="<%= en_about.keywords %>">
  <meta name="description" content="<%= en_about.description %>">
  <meta property="og:description" content="<%= en_about.description %>" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="<%= en_about.url %>" />
  <meta property="og:image" content="<%= en_about.image %>" />
  <meta property="og:site_name" content="<%= en_about.title %>" />
  <meta property="og:title" content="<%= en_about.title %>" />
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:title" content="<%= en_about.title %>">
  <meta name="twitter:description" content="<%= en_about.description %>">
  <meta name="twitter:image" content="<%= en_about.twitterImage %>">
  <% include ../../_inc/css_en %>
  <% include ../../_inc/link %>
  <% include ../../_inc/font %>
